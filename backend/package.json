{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "style": "eslint .",
    "style:fix": "npm run style -- --fix",
    "start": "node src/bin/www",
    "start:dev": "nodemon start",
    "test": "jest",
    "test:coverage" : "npm run test -- --coverage",
    "qualityGate": "npm run style && npm run test:coverage -- --coverageThreshold='{\"global\":{\"lines\":60}}'"
  },
  "devDependencies": {
    "babel-jest": "^26.6.3",
    "@commitlint/config-conventional": "^11.0.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "nodemon": "^2.0.6",
    "swagger-ui-express": "^4.1.6",
    "swagger-jsdoc": "^6.0.0"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.9.0",
    "mongoose": "^5.11.9",
    "morgan": "~1.9.1",
    "winston": "^3.3.3"
  }
}
